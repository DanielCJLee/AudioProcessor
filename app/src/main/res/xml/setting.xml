<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:example="http://schemas.android.com/apk/res/fruitbasket.com.audioprocessor"
    android:title="设置">
    <ListPreference
        android:key="VOICE_CHOICE"
        android:entries="@array/voice_choice"
        android:entryValues="@array/voice_choice_values"
        android:title="声道"
        android:dialogTitle="声道"
        android:defaultValue="2"
        />
    <ListPreference
        android:key="VOICE_TYPE"
        android:entries="@array/voice_type"
        android:entryValues="@array/voice_type_values"
        android:title="声波类型"
        android:dialogTitle="声波类型"
        android:defaultValue="0"
        />
    <fruitbasket.com.audioprocessor.ui.MySeekBarPreference
        android:defaultValue="0"
        android:dialogTitle="频率"
        android:key="HZ"
        android:title="频率"
        example:maxValue="20"
        example:minValue="0"/>
    <Preference
        android:key="RESET"
        android:title="清空"
        />
</PreferenceScreen>